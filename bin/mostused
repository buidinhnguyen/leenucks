#!/bin/sh
#
# most used apps via dmenu

IFS=$(printf '\t\n')
PATH=${PATH}:${HOME}/bin

# apps {{{
apps="chromium
dmenuclip
dmenurl
firefox
gimp
gvba
jameica
kb_switch.sh
leave.sh
libreoffice
lyx
mnb
openttd
pioneers
pysol
urxvt
urxvt-dvtm
virtualbox
vms
widelands"
# }}}

$(echo ${apps} |\
 dmenu -i -fn 'Proggyoptis-8' -nb '#cccccc' -nf '#000000' -sb '#cccccc' -sf '#0066ff' )

