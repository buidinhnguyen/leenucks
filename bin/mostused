#!/bin/zsh
#
# most used apps via dmenu

IFS=$'\t\n'
PATH=${PATH}:${HOME}/bin

# apps {{{
apps="dmenuclip
dmenurl
gajim
gimp
google-chrome
gvba
kb_switch.sh
leave.sh
libreoffice
lyx
openttd
urxvt
urxvt-dvtm
st
st-dvtm
virtualbox
vms
widelands"
# }}}

$(echo ${apps} |\
 dmenu -i -fn 'Proggyoptis-8' -nb '#cccccc' -nf '#000000' -sb '#cccccc' -sf '#0066ff' )

